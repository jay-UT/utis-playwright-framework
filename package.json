{
  "name": "henryscheinwebbdd",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
  "test": "playwright test",
  "test:ci:regression": "playwright test --grep @regression",

  "purge": "powershell -Command \"Get-ChildItem -Directory -Filter 'allure-report-*' | Where-Object { $_.LastWriteTime -lt (Get-Date).AddDays(-30) } | Remove-Item -Recurse -Force -ErrorAction SilentlyContinue\"",

  "clean": "powershell -Command \"if (Test-Path allure-results) { Remove-Item -Recurse -Force allure-results\\* }\"",

  "allure:env": "powershell -Command \"'[Environment]`nOS=Windows 11`nTester=' + $env:USERNAME | Out-File -FilePath allure-results/environment.properties -Encoding utf8\"",

  "allure:timestamp": "powershell -Command \"...\"",

  "full-report": "npm run purge && npm run clean && (npm run test || true) && npm run allure:env && npm run allure:timestamp"
}
,
  "bin": {
  "full-report": "./scripts/full-report.js"
},

  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@types/node": "^25.0.9",
    "allure-commandline": "^2.36.0",
    "allure-playwright": "^3.4.5"
  },
  "dependencies": {
    "@faker-js/faker": "^10.2.0",
    "csv-parse": "^6.1.0",
    "xlsx": "^0.18.5"
  }
}
